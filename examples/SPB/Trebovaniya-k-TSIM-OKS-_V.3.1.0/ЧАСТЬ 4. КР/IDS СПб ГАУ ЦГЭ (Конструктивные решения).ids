          <baseName>
            <simpleValue>Класс пожарной опасности</simpleValue>
          </baseName>
          <value>
            <xs:restriction base="xs:string">
              <xs:enumeration value="К0" />
              <xs:enumeration value="К1" />
              <xs:enumeration value="К2" />
              <xs:enumeration value="К3" />
              <xs:enumeration value="н/н" />
            </xs:restriction>
